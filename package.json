{
  "name": "file-listing-service",
  "version": "0.0.0",
  "description": "File listing REST API server",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ignore-path .eslintignore --ext .ts",
    "lint:fix": "eslint . --ignore-path .eslintignore --ext .ts --fix",
    "format:fix": "prettier --write \"./**/*.{ts,js,json,md}\"",
    "format:check": "prettier --list-different \"./**/*.{ts,js,json,md}\"",
    "start": "ts-node src/main.ts",
    "start:dev": "ts-node-dev --respawn src/main.ts"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "fastify": "^4.24.3"
  },
  "keywords": [
    "node",
    "rest",
    "typescript",
    "fastify",
    "eslint",
    "prettier"
  ],
  "author": "Paul Shpilsher <paul.shpilsher@gmail.com>",
  "license": "ISC"
}
